tTime()\n\n  for (let i = 0; i < 10; i++) {\n    sourceData.push({\n      x: moment(new Date(beginDay + 1000 * 60 * 60 * 24 * i)).format('YYYY-MM-DD'),\n      y: Math.round(Math.random() * 10)\n    })\n  }\n\n  const tooltip = [\n    'x*y',\n    (x, y) => ({\n      name: x,\n      value: y\n    })\n  ]\n\n  const scale = [{\n    dataKey: 'x',\n    min: 2\n  }, {\n    dataKey: 'y',\n    title: '时间',\n    min: 1,\n    max: 30\n  }]\n\n  export default {\n    name: 'MiniBar',\n    props: {\n      dataSource: {\n        type: Array,\n        default: () => []\n      },\n      width: {\n        type: Number,\n        default: null\n      },\n      height: {\n        type: Number,\n        default: 200\n      }\n    },\n    created() {\n      if (this.dataSource.length === 0) {\n        this.data = sourceData\n      } else {\n        this.data = this.dataSource\n      }\n    },\n    data() {\n      return {\n        tooltip,\n        data: [],\n        scale\n      }\n    }\n  }\n</script>\n\n<style lang=\"scss\" scoped>\n  @import \"chart\";\n</style>"]}]}