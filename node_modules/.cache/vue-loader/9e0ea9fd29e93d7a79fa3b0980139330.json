e=\"getTitle\" :link-list=\"linkList\" :search=\"search\" :tabs=\"tabs\">\n    <div slot=\"extra\" class=\"extra-img\">\n      <img :src=\"extraImage\"/>\n    </div>\n    <!-- keep-alive  -->\n    <route-view ref=\"content\"></route-view>\n  </page-layout>\n</template>\n\n<script>\n  import PageLayout from '../page/PageLayout'\n  import RouteView from './RouteView'\n\n  export default {\n    name: \"PageContent\",\n    components: {\n      RouteView,\n      PageLayout\n    },\n    data () {\n      return {\n        title: '',\n        description: '',\n        linkList: [],\n        extraImage: '',\n        search: false,\n        tabs: {}\n      }\n    },\n    mounted () {\n      this.getPageHeaderInfo()\n    },\n    updated () {\n      this.getPageHeaderInfo()\n    },\n    computed: {\n\n      getTitle () {\n        return this.$route.meta.title\n      }\n\n    },\n    methods: {\n      getPageHeaderInfo () {\n        // eslint-disable-next-line\n        this.title = this.$route.meta.title\n        // 因为套用了一层 route-view 所以要取 ref 对象下的子节点的第一个对象\n        const content = this.$refs.content && this.$refs.content.$children[0]\n\n        if (content) {\n          this.description = content.description\n          this.linkList = content.linkList\n          this.extraImage = content.extraImage\n          this.search = content.search == true ? true : false\n          this.tabs = content.tabs\n        }\n      }\n    }\n  }\n</script>\n\n<style lang=\"scss\" scoped>\n  .extra-img {\n    margin-top: -60px;\n    text-align: center;\n    width: 195px;\n\n    img {\n      width: 100%;\n    }\n  }\n\n  .mobile {\n    .extra-img{\n      margin-top: 0;\n      text-align: center;\n      width: 96px;\n\n      img{\n        width: 100%;\n      }\n    }\n  }\n</style>"]}]}